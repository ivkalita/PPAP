<div class="modal fade" id="user-card" data-url="/profile/user" tabindex="-1" role="dialog" aria-labelledby="user-card-label" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title" id="user-card-label"><%- Helper.translate(req, 'ejs-user-card-label') %></h4>
			</div>
			<div class="modal-body">
				<form>
					<div class="form-group">
						<label class="control-label" for="user-card-firstName-text"><%- Helper.translate(req, 'ejs-user-card-firstName-text') %></label>
						<input class="form-control" data-attr="firstName" type="text" id="user-card-firstName-text" />
					</div>
					<div class="form-group">
						<label class="control-label" for="user-card-lastName-text"><%- Helper.translate(req, 'ejs-user-card-lastName-text') %></label>
						<input class="form-control" data-attr="lastName" type="text" id="user-card-lastName-text" />
					</div>
					<div class="form-group">
						<label class="control-label" for="user-card-middleName-text"><%- Helper.translate(req, 'ejs-user-card-middleName-text') %></label>
						<input class="form-control" data-attr="middleName" type="text" id="user-card-middleName-text" />
					</div>
					<div class="form-group">
						<label class="control-label" for="user-card-birthday-text"><%- Helper.translate(req, 'ejs-user-card-birthday-text') %></label>
						<input class="form-control" data-attr="birthday" type="date" id="user-card-birthday-text" />
					</div>
					<div class="form-group">
						<label class="control-label" for="user-card-password-text"><%- Helper.translate(req, 'ejs-password') %></label>
						<input class="form-control" data-attr="password" type="text" id="user-card-password-text" />
					</div>
				</form>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default cancel" data-type="user" data-attr="id" data-dismiss="modal"><%- Helper.translate(req, 'ejs-btn-cancel') %></button>
				<button type="button" class="btn btn-primary save" data-type="user" data-attr="id"><%- Helper.translate(req, 'ejs-btn-save') %></button>
			</div>
		</div>
	</div>
</div>